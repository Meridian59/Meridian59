        BEGIN
            MENUITEM "&Next Chapter",               ID_PLAYBACK_NEXTCHAPTER
            MENUITEM "&Previous Chapter",           ID_PLAYBACK_PREVIOUSCHAPTER

            MENUITEM SEPARATOR
