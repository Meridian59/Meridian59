Table is PassiveObject

constants:

   include blakston.khd

resources:

   table_icon_rsc = table.bgf
   table_name_rsc = "table"
   table_desc_rsc = "This is a wooden table, weathered from years of use and abuse."

classvars:

   vrName = table_name_rsc
   vrIcon = table_icon_rsc
   vrDesc = table_desc_rsc

properties:


messages:

   GetObjectFlags()
   {
      return MOVEON_NO + LOOK_NO;
   }

end
