RatTreasure is TreasureType

constants:
   
   include blakston.khd
   
classvars:


properties:
   
   piTreasure_num = TID_RAT

messages:
   
   constructed()
   {
      plTreasure = [ [ &Herbs, 30 ],
                     [ &Mushroom, 20],
                     [ &Elderberry, 15 ],
                     [ &Snack, 15],
                     [ &Money, 10],
                     [ &BlueMushroom, 5],
                     [ &RedMushroom, 5 ] ];

      propagate;
   }

end
